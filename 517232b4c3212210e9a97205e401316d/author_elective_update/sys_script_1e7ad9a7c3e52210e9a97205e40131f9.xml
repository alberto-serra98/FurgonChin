        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
    // Controlla che la prenotazione sia appena stata annullata
    if (current.stato_prenotazione == 'Annullata' && previous.stato_prenotazione != 'Annullata') {
        
        // Se assicurata è true (può essere 'true', true, o '1')
        var isAssicurata = current.assicurata == true || current.assicurata == 'true' || current.assicurata == '1';

        if (isAssicurata) {
            current.costo_totale = 0;
        }
    }
})(current, previous);

