        <name>Controllo Credito Utente</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var modelloId = g_form.getValue('modello');
    var dataInizio = g_form.getValue('data_prenotazione');
    var dataFine = g_form.getValue('data_restituzione');

    if (!modelloId || !dataInizio || !dataFine) {
        g_form.setValue('credito_valido', 'false');
        return;
    }

    var ga = new GlideAjax('ControlloCreditoUtente');
    ga.addParam('sysparm_name', 'verificaCredito');
    ga.addParam('sysparm_modello_id', modelloId);
    ga.addParam('sysparm_data_prenotazione', dataInizio);
    ga.addParam('sysparm_data_restituzione', dataFine);

    ga.getXMLAnswer(function(response) {
        if (response.startsWith('CREDITO_INSUFFICIENTE')) {
            g_form.showFieldMsg(
                'modello',
                'Credito insufficiente per effettuare questa prenotazione.',
                'error'
            );
            g_form.setValue('credito_valido', 'false');
        } else {
            g_form.setValue('credito_valido', 'true');
        }
    });
}
]]></script>
